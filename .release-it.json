{
  "git": {
    "commitMessage": "v${version}",
    "tagName": "v${version}",
    "tagAnnotation": "v${version}"
  },
  "github": {
    "release": true,
    "releaseName": "v${version}"
  },
  "npm": {
    "publish": false
  },
  "plugins": {
    "@release-it/bumper": {
      "out": ["web/package.json", "web/package-lock.json"]
    }
  }
}
